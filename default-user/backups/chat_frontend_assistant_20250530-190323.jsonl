ound-color: var(--twitch-purple-hover);\n        }\n\n\n        /* Main Content Area */\n        .main-content {\n            display: flex;\n            flex-grow: 1; /* Allow main content to fill remaining space */\n            overflow: hidden; /* Prevent main content from overflowing body */\n        }\n\n        /* Stream Area */\n        .stream-area {\n            flex-grow: 1;\n            background-color: var(--dark-bg);\n            display: flex;\n            flex-direction: column;\n            padding: 1rem;\n            overflow-y: auto; /* Scroll if content overflows */\n        }\n\n        .video-player-placeholder {\n            width: 100%;\n            aspect-ratio: 16 / 9;\n            background-color: #000000;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            color: var(--text-secondary);\n            font-size: 1.5rem;\n            border: 1px solid var(--border-color);\n            margin-bottom: 1rem;\n        }\n\n        .stream-info {\n            background-color: var(--primary-bg);\n            padding: 1rem;\n            border-radius: 4px;\n            border: 1px solid var(--border-color);\n        }\n\n        .stream-info-header {\n            display: flex;\n            align-items: flex-start;\n            margin-bottom: 0.75rem;\n        }\n        .streamer-avatar {\n             width: 50px;\n             height: 50px;\n             border-radius: 50%;\n             background-color: var(--tertiary-bg); /* Placeholder */\n             margin-right: 1rem;\n             flex-shrink: 0;\n             background-size: cover;\n             background-position: center;\n        }\n        .streamer-details h2 {\n            font-size: 1.25rem;\n            color: var(--text-primary);\n            margin-bottom: 0.25rem;\n        }\n        .streamer-details .stream-title {\n            font-size: 1rem;\n            color: var(--text-primary);\n            margin-bottom: 0.25rem;\n            font-weight: normal;\n        }\n        .streamer-details .game-category {\n            font-size: 0.9rem;\n            color: var(--link-color);\n            cursor: pointer;\n        }\n        .streamer-details .game-category:hover {\n            text-decoration: underline;\n        }\n\n        .stream-actions {\n            margin-left: auto; /* Pushes buttons to the right */\n            display: flex;\n            gap: 0.5rem;\n        }\n\n        .action-button {\n            background-color: var(--twitch-purple);\n            color: var(--button-text);\n            border: none;\n            padding: 0.5rem 1rem;\n            border-radius: 4px;\n            cursor: pointer;\n            font-size: 0.9rem;\n            font-weight: bold;\n        }\n        .action-button:hover {\n            background-color: var(--twitch-purple-hover);\n        }\n        .action-button.secondary {\n            background-color: var(--secondary-bg);\n            color: var(--text-primary);\n        }\n        .action-button.secondary:hover {\n            background-color: var(--tertiary-bg);\n        }\n\n        .stream-tags {\n            margin-top: 0.75rem;\n            display: flex;\n            flex-wrap: wrap;\n            gap: 0.5rem;\n        }\n        .tag {\n            background-color: var(--secondary-bg);\n            color: var(--text-secondary);\n            padding: 0.25rem 0.5rem;\n            border-radius: 10px;\n            font-size: 0.75rem;\n        }\n\n\n        /* Chat Area */\n        .chat-area {\n            width: 340px; /* Twitch's default chat width */\n            flex-shrink: 0; /* Prevent chat from shrinking */\n            background-color: var(--primary-bg);\n            border-left: 1px solid var(--border-color);\n            display: flex;\n            flex-direction: column;\n            height: 100%; /* Fill parent height */\n        }\n\n        .chat-header {\n            padding: 0.75rem;\n         